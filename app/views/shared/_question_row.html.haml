%tr
  %td= question.title
  - unless question.audio.url.blank?
    %td= link_to "Open", question.audio.url, :target => "_blank"
  - else
    %td No Audio

    
  %td= question.type
- if question.class == MultipleChoiceQuestion
  - question.choices.each do |choice|
    %tr
      %td.sub{:colspan => "3"} #{choice.question} | #{choice.correct} 
